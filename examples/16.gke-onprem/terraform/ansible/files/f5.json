{
    "Annotation": "BIG-IP Local Traffic Manager Virtual Edition\n    Copyright 2009-2016 F5 Networks (http://www.f5.com)\n    \n    For support please visit http://support.f5.com",
    "DiskProvisioning": "thin",
    "IPAllocationPolicy": "dhcpPolicy",
    "IPProtocol": "IPv4",
    "InjectOvfEnv": true,
    "MarkAsTemplate": false,
    "Name": "F5 BIG-IP LTM",
    "PropertyMapping": [
        {
            "Key": "net.mgmt.addr",
            "Value": "172.16.10.4/24"
        },
        {
            "Key": "net.mgmt.gw",
            "Value": "172.16.10.1"
        }
    ],
    "NetworkMapping": [
        {
            "Name": "internal management",
            "Network": "internal management"
        },
        {
            "Name": "internal vm network",
            "Network": "internal vm network"
        },
        {
            "Name": "external vm network",
            "Network": "external vm network"
        }
    ],
    "PowerOn": true,
    "WaitForIP": true
}